<p id="notice"><%= notice %></p>

<p>
  <strong>Nombre:</strong>
  <%= @usuario.nombre %>
</p>

<p>
  <strong>Apellido:</strong>
  <%= @usuario.apellido %>
</p>

<p>
  <strong>Edad:</strong>
  <%= @usuario.edad %>
</p>

<p>
  <strong>Email:</strong>
  <%= @usuario.email %>
</p>



<p>
  <strong>Rol:</strong>
  <td>  
      <% @rols.each do |rol| %>
          <% if rol.id == @usuario.rol_id %>
              <%= rol.nombre %>
          <% end %>
      <% end %> 
  </td>

</p>

<%= link_to 'Edit', edit_usuario_path(@usuario) %> |
<%= link_to 'Back', usuarios_path %>
